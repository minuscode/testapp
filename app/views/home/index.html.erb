<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<table>
  <tr>
    <th>id</th>
    <th>jobid</th>
    <th>provider</th>
    <th>title</th>
    <th>description</th>
    <th>pubdate</th>
    <th>link</th>
    <th>categories</th>
  </tr>

<% @jobs.each do |job| %>
  <tr>
    <td><%= job.id %></td>
    <td><%= job.jobid %></td>
    <td><%= job.provider %></td>
    <td><%= job.title %></td>
    <td><%= job.description %></td>
    <td><%= job.pubdate %></td>
    <td><a href="<%= job.link %>">link</a></td>
    <td><%= job.categories %></td>
  </tr>
<% end %>